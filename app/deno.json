{
  "tasks": {
    "dev": "deno task dev:api & deno task dev:vite",
    "dev:vite": "deno run -A --node-modules-dir npm:vite",
    "dev:api": "deno run -A --watch api/main.ts",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview"
  },
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.0",
    "@gz/jwt": "jsr:@gz/jwt@^0.1.0",
    "@momiji/cors": "jsr:@momiji/cors@^1.0.1",
    "@oak/oak": "jsr:@oak/oak@^17.1.0",
    "@supabase/supabase-js": "npm:@supabase/supabase-js@^2.47.1",
    "@tajpouria/cors": "jsr:@tajpouria/cors@^1.2.1",
    "@ts-rex/bcrypt": "jsr:@ts-rex/bcrypt@^1.0.3",
    "@types/react": "npm:@types/react@^18.3.1",
    "@types/react-dom": "npm:@types/react-dom@^18.3.1",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.2",
    "autoprefixer": "npm:autoprefixer@^10.4.20",
    "jwt-decode": "npm:jwt-decode@^4.0.0",
    "lucide-react": "npm:lucide-react@^0.464.0",
    "postcss": "npm:postcss@^8.4.47",
    "react": "npm:react@^18.3.1",
    "react-dom": "npm:react-dom@^18.3.1",
    "react-router-dom": "npm:react-router-dom@^6.27.0",
    "tailwindcss": "npm:tailwindcss@^3.4.14",
    "vite": "npm:vite@^5.4.8"
  }
}
